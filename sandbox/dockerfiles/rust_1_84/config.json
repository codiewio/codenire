{
  "Template": "rust_1_84",
  "Groups": ["rust"],
  "Workdir": "/app",
  "Enabled": true,
  "Connections": ["postgres"],
  "ContainerOptions": {
    "CompileTTL": 30,
    "RunTTL": 5
  },
  "IsSupportPackage": true,

  "Actions": {
    "cargo": {
      "Id": "cargo",
      "Name": "Rust 1.84 (Cargo)",
      "CompileCmd": "cargo -v",
      "RunCmd": "cargo run {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.rs"
      },
      "DefaultFiles": {}
    }
  }
}
