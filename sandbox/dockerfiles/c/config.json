{
  "Template": "c",
  "Groups": ["c"],
  "Enabled": true,
  "Connections": ["postgres"],
  "ContainerOptions": {
    "CompileTTL": 30,
    "RunTTL": 5
  },
  "IsSupportPackage": true,

  "Actions": {
    "default": {
      "Id": "default",
      "IsDefault": true,
      "Name": "C23",
      "CompileCmd": "g++ -std=c23 $(find . -name '*.c') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.c"
      }
    },

    "c17": {
      "Id": "c17",
      "IsDefault": false,
      "Name": "C17(18)",
      "CompileCmd": "g++ -std=c17 $(find . -name '*.c') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.c"
      }
    },

    "c11": {
      "Id": "c11",
      "IsDefault": false,
      "Name": "C11",
      "CompileCmd": "g++ -std=c11 $(find . -name '*.c') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.c"
      }
    },

    "c++14": {
      "Id": "default",
      "IsDefault": false,
      "Name": "C++14",
      "CompileCmd": "g++ -std=c++14 $(find . -name '*.cpp') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.cpp"
      }
    },

    "c++11": {
      "Id": "c++11",
      "IsDefault": false,
      "Name": "C++11",
      "CompileCmd": "g++ -std=c++11 $(find . -name '*.cpp') -o main ",
      "RunCmd": "./main {ARGS} < {STDIN}",
      "ScriptOptions": {
        "SourceFile": "main.cpp"
      }
    }
  }
}
